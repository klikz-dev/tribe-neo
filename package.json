{
  "name": "tribe-neo",
  "version": "5.2.3",
  "main": "index.js",
  "repository": "git@gitlab.com:tribeplatform/tribe-neo.git",
  "packageManager": "yarn@3.1.1",
  "author": "Kamran Ghoochanian <kam@tribe.so>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^16.13.0",
    "yarn": "3.1.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "react-sdk": "yarn workspace @tribeplatform/react-sdk",
    "react-ui-kit": "yarn workspace @tribeplatform/react-ui-kit",
    "web": "yarn workspace @tribeplatform/web",
    "gql-client": "yarn workspace @tribeplatform/gql-client",
    "slate-kit": "yarn workspace @tribeplatform/slate-kit",
    "dev-portal": "yarn workspace @tribeplatform/dev-portal",
    "portal": "yarn workspace @tribeplatform/portal",
    "prepare": "husky install",
    "bootstrap": "lerna bootstrap",
    "commitlint": "commitlint",
    "sync": "git pull && yarn && yarn clean && yarn build:deps",
    "clean": "lerna run --parallel clean",
    "build:deps": "run-p \"gql-client build\" \"react-ui-kit build\" \"slate-kit build\" && run-p \"react-sdk build\"",
    "lint": "yarn workspaces run lint",
    "lint:eslint:fix": "eslint --fix",
    "dev": "yarn web start --inspect",
    "dev:watch": "lerna run --parallel build:watch",
    "storybook": "yarn react-ui-kit storybook",
    "test": "run-p \"gql-client test\" \"react-ui-kit test\"",
    "build": "yarn build:deps && yarn web build",
    "publish": "lerna publish --conventional-commits",
    "cm": "cz",
    "build:min": "echo 'This command is deprecated. Use `build:deps` instead.' && yarn build:deps "
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.172",
    "@types/lodash.merge": "^4.6.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/serialize-javascript": "^5.0.1",
    "@types/tailwindcss": "^2.2.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.0.6",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.3",
    "tsc": "^2.0.3",
    "typescript": "^4.4.2",
    "vite": "^2.7.2",
    "vite-tsconfig-paths": "^3.3.17",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-syntax-highlighter": "^15.4.5",
    "should-send-same-site-none": "^2.0.5"
  }
}
